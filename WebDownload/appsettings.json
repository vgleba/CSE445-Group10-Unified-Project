{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Download": {
    "TimeoutSeconds": 10,
    "MaxBytes": 1048576,
    "AllowedSchemes": [ "http", "https" ],
    "BlockPrivateNetworks": true,
    "ReturnPlainTextByDefault": true
  }
}
